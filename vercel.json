{
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "app.js": {
        "maxDuration": 30
      }
    },
    "env": {
      "NODE_ENV": "production"
    },
    "build": {
      "env": {
        "NODE_ENV": "production",
        "NODE_VERSION": "18"
      }
    },
    "installCommand": "npm install",
    "buildCommand": "npm run build"
  }
  