# Variables de entorno necesarias para Vercel
# Configura estas variables en el dashboard de Vercel

# Configuración de la API
API_PREFIX=/api
NODE_ENV=production

# Configuración de la base de datos PostgreSQL (producción)
PG_HOST=tu_host_produccion
PG_PORT=5432
PG_DATABASE=tu_database_produccion
PG_USERNAME=tu_usuario_produccion
PG_PASSWORD=tu_password_produccion

# Configuración CORS (para producción)
FRONTEND_URL=https://justhome-frontend.vercel.app

# Configuración adicional para Vercel
VERCEL=true 